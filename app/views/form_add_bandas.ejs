<!DOCTYPE HTML>
<html lang="pt-br">
  <head>
    <meta charset="utf-8">
    <link rel="stylesheet" href="css/style.css">
    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">

    <!-- Optional theme -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap-theme.min.css" integrity="sha384-rHyoN1iRsVXV4nD0JutlnGaslCJuC7uwjduW9SVrLvRYooPp2bWYgmgJQIXwl/Sp" crossorigin="anonymous">

    <!-- Latest compiled and minified JavaScript -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>

    <!-- Material icons -->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  </head>
  <body>
      <nav class="navbar navbar-inverse">
          <div class="container-fluid">
            <div class="navbar-header">
              <a class="navbar-brand" href="#">S2M Music <i class="material-icons navbar-icon">music_note</i></a>
            </div>
            <ul class="nav navbar-nav">
              <li class="active"><a href="#">Home</a></li>
              <li><a href="#">Sortear</a></li>
              <li><a href="/cadastros">Cadastrar</a></li>
              <li><a href="#">Histórico</a></li>
            </ul>
        </div>
      </nav>
      <form class="form-horizontal" action="/cadastrarbanda" method="POST">
          <div class="form-group">
            <label class="control-label col-sm-2" for="banda">Banda:</label>
            <div class="col-sm-6">
              <input type="text" class="form-control" id="nome" name="nome" placeholder="Nome da banda">
            </div>
          </div>
          <div class="form-group">
            <label class="control-label col-sm-2" for="genero">Gênero:</label>
            <div class="col-sm-6">
              <select id="genero" name="genero">
                <% for(var i = 0; i < generos.length; i++) {%>
                  <option value="<%=generos[i].id%>"><%=generos[i].genero%></option>
                <% } %>
              </select>
            </div>
          </div>
          <div class="form-group">
          </div>
          <div class="form-group">
            <div class="col-sm-offset-2 col-sm-10">
              <button type="submit" class="btn btn-default">Submit</button>
            </div>
          </div>
        </form>
  </body>
</html>